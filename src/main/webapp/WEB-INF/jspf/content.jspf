<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<div id="container">
    <div class="items-container">
        <c:forEach  var="item" items="${items}">
            <div class="item">
                <span>${item.nombre}</span>
                <img src="${item.imgUrl}">
                <span>${item.precio}</span>
            </div>
        </c:forEach>
    </div>
</div>